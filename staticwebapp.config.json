{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/*.{css,js,png,jpg,gif,svg}"]
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html",
        "statusCode": 200
      }
    },
    "mimeTypes": {
      ".js": "text/javascript",
      ".css": "text/css"
    }
  }